<appSettings>
  <add key="startingGems" value="7775000" /><!-- Beware, very high value will crash UCS -->
  <add key="startingGold" value="100000000" /><!-- Beware, very high value will crash UCS -->
  <add key="startingElixir" value="500000000" /><!-- Beware, very high value will crash UCS -->
  <add key="startingDarkElixir" value="1000000" /><!-- Beware, very high value will crash UCS -->
  <add key="startingTrophies" value="4000" />
  <add key="startingShieldTime" value="0" /><!-- Initial shield duration in seconds -->

  <add key="startingLevel" value="1" /><!-- User start Level (Beware,9 is the highest starting level) -->
  <add key="startingExperience" value="random" /><!-- Set to random for random - User start experience -->

  <add key="clientVersion" value="8.212" /><!-- Major version dot Client build -->
  <add key="UpdateUrl" value="http://ultrapowa.com/" />
  <!-- Specify where to redirect if the CoC version is outdated -->
  <add key="useCustomPatch" value="false" /><!-- Specify if the server is using patches (.csv, .sc, ...) -->
  <add key="patchingServer" value="http://178.32.9.216/" /><!-- Don't forget the trailing slash -->
  <add key="maintenanceMode" value="false" />
  <!-- People trying to connect will see a maintenance message if set to true -->
  <add key="maintenanceTimeleft" value="1200" />
  <!-- Please set the time in seconds like 1200 seconds for 20 minutes -->
  <add key="databaseConnectionName" value="ucsdbEntities" />
  <!-- Choose ucsdbEntities for MySql, and edit ucsdbEntities connection string in ucs.exe.config -->
  <add key="loggingLevel" value="1" /><!-- Enable logging for debugging, set value to 1 for (almost) no log -->
  <add key="proDebugPort" value="8888" /><!-- Choose the port for Pro Api Manager, by default set value to 1173 -->
  <add key="ApiKey" value="UCSGL" />
  <add key="ServerCapacityMode" value="false" /><!-- Enable Server Capacity or disable it -->
  <add key="ServerCapacity" value="1000" /><!-- The Amount of players which are allowed to play -->
  <add key="ServerFullMessage" value="The Server is currently full! Please try again later." />
  <!-- The Message which should be displayed if the server is full -->
</appSettings>

